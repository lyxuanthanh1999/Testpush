@model FeedBackCreateRequest
<!--====== Section 1 ======-->
<div class="u-s-p-y-30">

    <!--====== Section Content ======-->
    <div class="section__content">
        <div class="container">
            <div class="breadcrumb">
                <div class="breadcrumb__wrap">
                    <ul class="breadcrumb__list">
                        <li class="has-separator">

                            <a href="/">Trang chủ</a>
                        </li>
                        <li class="is-marked">

                            <a href="#">Liên hệ</a>
                        </li>
                    </ul>
                </div>
            </div>
        </div>
    </div>
</div>
<!--====== End - Section 1 ======-->
<!--====== Section 2 ======-->
<div class="u-s-p-b-60">

    <!--====== Section Content ======-->
    <div class="section__content">
        <div class="container">
            <div class="row">
                <div class="col-lg-12">
                    <div class="g-map">
                        <div id="map"></div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <!--====== End - Section Content ======-->
</div>
<!--====== End - Section 2 ======-->
<!--====== Section 3 ======-->
<div class="u-s-p-b-60">

    <!--====== Section Content ======-->
    <div class="section__content">
        <div class="container">
            <div class="row">
                <div class="col-lg-4 col-md-6 u-s-m-b-30">
                    <div class="contact-o u-h-100">
                        <div class="contact-o__wrap">
                            <div class="contact-o__icon"><i class="fas fa-phone-volume"></i></div>

                            <span class="contact-o__info-text-1">Số điện thoại hỗ trợ</span>

                            <span class="contact-o__info-text-2">(+84) 123 456 789</span>

                            <span class="contact-o__info-text-2">(+0) 987 654 321</span>
                        </div>
                    </div>
                </div>
                <div class="col-lg-4 col-md-6 u-s-m-b-30">
                    <div class="contact-o u-h-100">
                        <div class="contact-o__wrap">
                            <div class="contact-o__icon"><i class="fas fa-map-marker-alt"></i></div>

                            <span class="contact-o__info-text-1">Địa chỉ</span>

                            <span class="contact-o__info-text-2">140 Lê Trọng Tấn, Tây Thạnh, Tân Phú</span>

                            <span class="contact-o__info-text-2">Thành phố Hồ Chí Minh</span>
                        </div>
                    </div>
                </div>
                <div class="col-lg-4 col-md-6 u-s-m-b-30">
                    <div class="contact-o u-h-100">
                        <div class="contact-o__wrap">
                            <div class="contact-o__icon"><i class="far fa-clock"></i></div>

                            <span class="contact-o__info-text-1">Thời gian làm việc</span>

                            <span class="contact-o__info-text-2">Từ thứ hai đến thứ sáu</span>

                            <span class="contact-o__info-text-2">từ 8 giờ sáng đến 5 giờ chiều</span>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <!--====== End - Section Content ======-->
</div>
<!--====== End - Section 3 ======-->
<!--====== Section 4 ======-->
<div class="u-s-p-b-60">

    <!--====== Section Content ======-->
    <div class="section__content">
        <div class="container">
            <div class="row">
                <div class="col-lg-12">
                    <div class="contact-area u-h-100">
                        <div class="contact-area__heading">
                            <h2>Gửi thông tin phản hồi</h2>
                        </div>
                        <form class="contact-f" method="post">
                            <div class="row">
                                <div class="col-lg-6 col-md-6 u-h-100">
                                    <div asp-validation-summary="All" class="text-error u-s-m-b-15"></div>
                                    <div class="u-s-m-b-15">

                                        <label for="c-name">Họ và tên:</label>

                                        <input class="input-text input-text--border-radius input-text--primary-style" type="text" asp-for="Name" id="c-name" placeholder="Tên (Bắt buộc)" required>
                                        <span asp-validation-for="Name" class="text-error"></span>
                                    </div>
                                    <div class="u-s-m-b-15">

                                        <label for="c-email">Email:</label>
                                        <input class="input-text input-text--border-radius input-text--primary-style" type="email" asp-for="Email" id="c-email" placeholder="Email (Bắt buộc)" required>
                                        <span asp-validation-for="Email" class="text-error"></span>
                                    </div>
                                    <div class="u-s-m-b-15">
                                        <label for="c-subject">Số điện thoại:</label>
                                        <input class="input-text input-text--border-radius input-text--primary-style" type="number" asp-for="PhoneNumber" id="c-subject" placeholder="Số điện thoại (Bắt buộc)" required>
                                        <span asp-validation-for="PhoneNumber" class="text-error"></span>
                                    </div>
                                </div>
                                <div class="col-lg-6 col-md-6 u-h-100">
                                    <div class="u-s-m-b-15">
                                        <label for="c-message">Tin nhắn:</label>
                                        <textarea class="text-area text-area--border-radius text-area--primary-style" style="height: auto" id="c-message" rows="18" asp-for="Message" placeholder="Tin nhắn (Bắt buộc)" required></textarea>
                                        <span asp-validation-for="Message" class="text-error"></span>
                                    </div>
                                </div>
                                <div class="col-lg-12">
                                    <button class="btn btn--e-brand-b-2" type="submit">Gửi phản hồi</button>
                                </div>
                            </div>
                        </form>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <!--====== End - Section Content ======-->
</div>
<!--====== End - Section 4 ======-->